    "scripts": {
        "copy-bootstrap": "copy /Y node_modules/bootstrap/dist/js/bootstrap.js resources/assets/js",
        "copy-coreui-css": "copy /Y node_modules/@coreui/coreui/dist/css/coreui.css resources/assets/css/",
        "copy-coreui-js": "copy /Y node_modules/@coreui/coreui/dist/js/coreui.js resources/assets/js/",
        "copy-coreui-utilities": "copy /Y node_modules/@coreui/coreui/dist/js/coreui-utilities.js resources/assets/js/",
        "copy-fontawesome-css": "copy /Y node_modules/@fortawesome/fontawesome-free/css/all.css resources/assets/fontawesome/css/fontawesome.css",
        "copy-fontawesome-fonts": "xcopy /R /E node_modules/@fortawesome/fontawesome-free/webfonts/ resources/assets/fontawesome/",
        "copy-jquery": "copy /Y node_modules/jquery/dist/jquery.js resources/assets/js/",
        "copy-perfect-scrollbar-js": "copy /Y node_modules/perfect-scrollbar/dist/perfect-scrollbar.js resources/assets/js/",
        "copy-popper": "copy /Y node_modules/popper.js/dist/umd/popper.js resources/assets/js/",
        "copy-assets": "npm run copy-bootstrap && npm run copy-coreui-css && npm run copy-coreui-js && npm run copy-coreui-utilities && npm run copy-fontawesome-css && npm run copy-fontawesome-fonts && npm run copy-jquery && npm run copy-perfect-scrollbar-js && npm run copy-popper"
    }
